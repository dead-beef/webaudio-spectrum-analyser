{
  "npmScope": "webaudio-spectrum-analyser",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": "*",
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "projects": {
    "webaudio-spectrum-analyser-e2e": {
      "tags": ["scope:webaudio-spectrum-analyser-e2e"]
    },
    "webaudio-spectrum-analyser": {
      "tags": ["scope:webaudio-spectrum-analyser", "type:application", "platform:browser"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "runtimeCacheInputs": ["node -v", "echo $IMPORTANT_ENV_VAR"],
        "cacheDirectory": "/tmp/nx/webaudio-spectrum-analyser"
      }
    }
  }
}
