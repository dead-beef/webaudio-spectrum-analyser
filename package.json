{
  "name": "webaudio-spectrum-analyser",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "analyze:webaudio-spectrum-analyser": "npm run build:webaudio-spectrum-analyser:stats; npx webpack-bundle-analyzer ./dist/apps/webaudio-spectrum-analyser/stats-es2015.json",
    "build": "ng build",
    "build:codepen": "./tools/shell/build-codepen",
    "build:webaudio-spectrum-analyser:stats": "ng build --stats-json --project webaudio-spectrum-analyser --configuration analyze-bundle",
    "compodoc:generate": "compodoc -p ./apps/webaudio-spectrum-analyser/tsconfig.json",
    "compodoc:serve": "compodoc -p ./apps/webaudio-spectrum-analyser/tsconfig.json -s",
    "dep-graph": "nx dep-graph",
    "e2e": "ng e2e",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "preinstall": "rm -rf node_modules/*",
    "postinstall": "npx sort-package-json && npm run ngcc",
    "install:all": "./tools/shell/install all",
    "install:global": "./tools/shell/install global",
    "install:shellcheck": "./tools/shell/install shellcheck",
    "install:shellcheck:ci": "./tools/shell/install shellcheck ci",
    "lint": "npx npm-run-all -s nx:lint stylelint:all prettier:all:html",
    "lint:fix": "npx npm-run-all -s nx:lint:fix stylelint:all:fix prettier:all:html:fix",
    "ng": "ng",
    "ngcc": "ngcc",
    "ngcc:ci": "ngcc --async false",
    "nx": "nx",
    "nx:lint": "nx workspace-lint && ng lint",
    "nx:lint:fix": "nx workspace-lint && ng lint --fix",
    "prettier:all:html": "npm run prettier:apps:html && npm run prettier:libs:html",
    "prettier:all:html:fix": "npm run prettier:apps:html:fix && npm run prettier:libs:html:fix",
    "prettier:apps:html": "npx prettier -c apps/**/**/*.html",
    "prettier:apps:html:fix": "npx prettier -c --write apps/**/**/*.html",
    "prettier:libs:html": "npx prettier -c libs/**/**/*.html",
    "prettier:libs:html:fix": "npx prettier -c --write libs/**/**/*.html",
    "start": "ng serve",
    "stylelint": "npx stylelint --syntax scss",
    "stylelint:all": "npm run stylelint:apps && npm run stylelint:libs",
    "stylelint:all:fix": "npm run stylelint:apps:fix && npm run stylelint:libs:fix",
    "stylelint:apps": "npx stylelint \"./apps/**/*.scss\"",
    "stylelint:apps:fix": "npx stylelint \"./apps/**/*.scss\" --fix",
    "stylelint:libs": "npx stylelint \"./libs/**/*.scss\"",
    "stylelint:libs:fix": "npx stylelint \"./libs/**/*.scss\" --fix",
    "test": "ng test --codeCoverage=false",
    "test:coverage": "ng test --codeCoverage=true",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run affected:lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@angular/animations": "9.1.9",
    "@angular/common": "9.1.9",
    "@angular/compiler": "9.1.9",
    "@angular/core": "9.1.9",
    "@angular/forms": "9.1.9",
    "@angular/platform-browser": "9.1.9",
    "@angular/platform-browser-dynamic": "9.1.9",
    "@angular/router": "9.1.9",
    "@clr/angular": "3.1.3",
    "@clr/icons": "3.1.3",
    "@clr/ui": "3.1.3",
    "@ngneat/until-destroy": "7.1.6",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "compression": "1.7.4",
    "core-js": "3.6.5",
    "express": "4.17.1",
    "isomorphic-fetch": "2.2.1",
    "memo-decorator": "2.0.1",
    "rxjs": "~6.5.5",
    "simplebar": "5.2.0",
    "simplebar-angular": "2.2.0",
    "stats.js": "0.17.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.7",
    "@angular-devkit/core": "9.1.7",
    "@angular-devkit/schematics": "9.1.7",
    "@angular-eslint/builder": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.32",
    "@angular-eslint/template-parser": "0.0.1-alpha.32",
    "@angular/cli": "9.1.7",
    "@angular/compiler-cli": "9.1.9",
    "@angular/language-service": "9.1.9",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@nrwl/angular": "9.3.0",
    "@nrwl/cypress": "9.3.0",
    "@nrwl/eslint-plugin-nx": "9.3.0",
    "@nrwl/jest": "9.3.0",
    "@nrwl/node": "9.3.0",
    "@nrwl/storybook": "9.3.0",
    "@nrwl/workspace": "9.3.0",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/angular": "5.3.19",
    "@types/jest": "25.2.3",
    "@types/jsdom": "16.2.3",
    "@types/node": "14.0.11",
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "@typescript-eslint/parser": "3.1.0",
    "babel-loader": "8.1.0",
    "codelyzer": "~5.2.2",
    "commitizen": "4.1.2",
    "cypress": "4.7.0",
    "cypress-multi-reporters": "1.4.0",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-compat": "3.7.0",
    "eslint-plugin-deprecation": "1.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-rxjs": "0.0.2-beta.24",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-watch": "6.0.1",
    "husky": "4.2.5",
    "jest": "25.3.0",
    "jest-preset-angular": "8.1.3",
    "prettier": "2.0.5",
    "prettier-stylelint": "0.4.2",
    "sort-json": "2.0.0",
    "sort-package-json": "1.44.0",
    "stylelint": "13.6.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-prettier": "1.1.2",
    "ts-jest": "25.3.1",
    "ts-node": "8.10.2",
    "typescript": "3.8.3",
    "webpack-bundle-analyzer": "3.8.0"
  },
  "engines": {
    "node": ">=12.16.1",
    "npm": ">=6.14.4"
  }
}
