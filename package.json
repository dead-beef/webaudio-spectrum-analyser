{
  "name": "webaudio-spectrum-analyser",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "analyze:webaudio-spectrum-analyser": "npm run build:webaudio-spectrum-analyser:stats; npx webpack-bundle-analyzer ./dist/apps/webaudio-spectrum-analyser/stats-es2015.json",
    "build": "nx build",
    "build:codepen": "./tools/shell/build-codepen",
    "build:gh-pages": "nx build --prod --base-href /webaudio-spectrum-analyser/",
    "build:prod": "nx build --prod",
    "build:webaudio-spectrum-analyser:stats": "nx build --stats-json --project webaudio-spectrum-analyser --configuration analyze-bundle",
    "compodoc:generate": "compodoc -p ./apps/webaudio-spectrum-analyser/tsconfig.json",
    "compodoc:serve": "compodoc -p ./apps/webaudio-spectrum-analyser/tsconfig.json -s",
    "predeploy:gh-pages": "gh-pages-clean",
    "deploy:gh-pages": "gh-pages --dist ./dist --no-history",
    "e2e": "npx nx run-many --target=e2e --all",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "postinstall": "npx sort-package-json && npm run ngcc && ./tools/shell/decorate-angular-cli && ./tools/shell/patch-capacitor-android",
    "install:all": "./tools/shell/install all",
    "install:global": "./tools/shell/install global",
    "install:shellcheck": "./tools/shell/install shellcheck",
    "install:shellcheck:ci": "./tools/shell/install shellcheck ci",
    "lint": "npx npm-run-all -s nx:lint stylelint:all prettier:all:html",
    "lint:fix": "npx npm-run-all -s nx:lint:fix stylelint:all:fix prettier:all:html:fix",
    "ng": "nx",
    "ngcc": "ngcc",
    "ngcc:ci": "ngcc --async false",
    "nx": "nx",
    "nx:dep-graph": "nx dep-graph",
    "nx:help": "nx help",
    "nx:lint": "npx nx run-many --target=lint --all",
    "nx:lint:fix": "npx nx run-many --target=lint --all --fix",
    "prettier:all:html": "npm run prettier:apps:html ",
    "prettier:all:html:fix": "npm run prettier:apps:html:fix",
    "prettier:apps:html": "npx prettier -c apps/**/**/*.html",
    "prettier:apps:html:fix": "npx prettier -c --write apps/**/**/*.html",
    "prettier:libs:html": "npx prettier -c libs/**/**/*.html",
    "prettier:libs:html:fix": "npx prettier -c --write libs/**/**/*.html",
    "start": "nx serve",
    "stylelint": "npx stylelint --syntax scss",
    "stylelint:all": "npm run stylelint:apps",
    "stylelint:all:fix": "npm run stylelint:apps:fix",
    "stylelint:apps": "npx stylelint \"./apps/**/*.scss\"",
    "stylelint:apps:fix": "npx stylelint \"./apps/**/*.scss\" --fix",
    "stylelint:libs": "npx stylelint \"./libs/**/*.scss\"",
    "stylelint:libs:fix": "npx stylelint \"./libs/**/*.scss\" --fix",
    "test": "npx nx run-many --target=test --all --codeCoverage=false",
    "test:coverage": "npx nx run-many --target=test --all --codeCoverage=true",
    "workspace:cleanup": "rm -rf ./documentation/* ./coverage/* ./dist/*",
    "workspace:schematic": "nx workspace-schematic",
    "workspace:update:check": "ncu",
    "workspace:update:cleanup": "rm -rf migrations.json",
    "workspace:update:run-migrations": "nx migrate --run-migrations=migrations.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "true || npm run affected:lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "puppeteer_skip_chromium_download": true
  },
  "dependencies": {
    "@angular/animations": "11.0.5",
    "@angular/common": "11.0.5",
    "@angular/compiler": "11.0.5",
    "@angular/core": "11.0.5",
    "@angular/forms": "11.0.5",
    "@angular/platform-browser": "11.0.5",
    "@angular/platform-browser-dynamic": "11.0.5",
    "@angular/router": "11.0.5",
    "@capacitor/android": "2.4.5",
    "@clr/angular": "3.1.5",
    "@clr/icons": "3.1.5",
    "@clr/ui": "3.1.5",
    "@ngneat/until-destroy": "^8.0.3",
    "@ngxs/store": "3.7.1",
    "core-js": "3.8.1",
    "font-awesome": "4.7.0",
    "isomorphic-fetch": "3.0.0",
    "memo-decorator": "2.0.1",
    "rxjs": "6.6.3",
    "simplebar": "5.3.0",
    "simplebar-angular": "2.3.0",
    "stats.js": "0.17.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.1",
    "@angular-devkit/build-angular": "0.1100.5",
    "@angular-devkit/core": "11.0.5",
    "@angular-devkit/schematics": "11.0.5",
    "@angular-eslint/builder": "0.8.0-beta.7",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.7",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.7",
    "@angular-eslint/template-parser": "0.8.0-beta.7",
    "@angular/cli": "11.0.5",
    "@angular/compiler-cli": "11.0.5",
    "@angular/language-service": "11.0.5",
    "@capacitor/cli": "2.4.5",
    "@capacitor/core": "2.4.5",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@ngxs/logger-plugin": "3.7.1",
    "@ngxs/storage-plugin": "3.7.1",
    "@nrwl/angular": "11.0.18",
    "@nrwl/cli": "11.0.18",
    "@nrwl/cypress": "11.0.18",
    "@nrwl/eslint-plugin-nx": "11.0.18",
    "@nrwl/jest": "11.0.18",
    "@nrwl/node": "11.0.18",
    "@nrwl/storybook": "11.0.18",
    "@nrwl/tao": "11.0.18",
    "@nrwl/workspace": "11.0.18",
    "@storybook/addon-knobs": "^6.1.11",
    "@storybook/angular": "^6.1.11",
    "@types/jest": "26.0.19",
    "@types/jsdom": "16.2.5",
    "@types/node": "14.14.16",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "babel-loader": "8.2.2",
    "codelyzer": "6.0.1",
    "commitizen": "4.2.2",
    "cpp-dependency-loader": "^0.2.0",
    "cpp-wasm-loader": "0.9.0",
    "cypress": "6.2.0",
    "cypress-multi-reporters": "1.4.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-compat": "3.9.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-deprecation": "1.2.0",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-rxjs": "2.1.5",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-watch": "7.0.0",
    "gh-pages": "3.1.0",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "jest-canvas-mock": "2.3.0",
    "jest-cpp-wasm": "0.1.0",
    "jest-preset-angular": "8.3.2",
    "prettier": "2.2.1",
    "prettier-stylelint": "0.4.2",
    "sort-json": "2.0.0",
    "sort-package-json": "1.48.0",
    "stylelint": "13.8.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-prettier": "1.1.2",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.0.5",
    "webpack-bundle-analyzer": "4.3.0"
  },
  "engines": {
    "node": ">=14.8.0",
    "npm": ">=6.14.8"
  }
}
